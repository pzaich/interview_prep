{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "installedESLint": true,
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,

            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": 0,
        "semi": [
            "error",
            "never"
        ],
        "no-console": [
            "warn"
        ],
        "react/prop-types": 0
    },
    "globals": {
        "process": true,
        "exports": true,
        "module": true,
        "require": true,
        "FB": true
    },
    "root": true
}
